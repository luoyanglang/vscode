{
	"name": "deepseek",
	"displayName": "DeepSeek AI",
	"description": "DeepSeek AI 代码助手集成",
	"version": "1.0.0",
	"publisher": "vscode",
	"engines": {
		"vscode": "^1.107.0"
	},
	"categories": [
		"AI",
		"Programming Languages"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "DeepSeek",
			"properties": {
				"deepseek.apiKey": {
					"type": "string",
					"default": "",
					"description": "DeepSeek API Key",
					"order": 1
				},
				"deepseek.apiEndpoint": {
					"type": "string",
					"default": "https://api.deepseek.com/v1",
					"description": "DeepSeek API 端点",
					"order": 2
				},
				"deepseek.model": {
					"type": "string",
					"default": "deepseek-chat",
					"enum": [
						"deepseek-chat",
						"deepseek-coder"
					],
					"description": "DeepSeek 模型选择",
					"order": 3
				},
				"deepseek.enabled": {
					"type": "boolean",
					"default": true,
					"description": "启用 DeepSeek AI 助手",
					"order": 0
				}
			}
		},
		"commands": [
			{
				"command": "deepseek.chat",
				"title": "DeepSeek: 开始对话",
				"category": "DeepSeek"
			},
			{
				"command": "deepseek.explainCode",
				"title": "DeepSeek: 解释代码",
				"category": "DeepSeek"
			},
			{
				"command": "deepseek.generateCode",
				"title": "DeepSeek: 生成代码",
				"category": "DeepSeek"
			},
			{
				"command": "deepseek.optimizeCode",
				"title": "DeepSeek: 优化代码",
				"category": "DeepSeek"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "deepseek.explainCode",
					"when": "editorHasSelection",
					"group": "deepseek@1"
				},
				{
					"command": "deepseek.optimizeCode",
					"when": "editorHasSelection",
					"group": "deepseek@2"
				}
			]
		},
		"keybindings": [
			{
				"command": "deepseek.chat",
				"key": "ctrl+shift+d",
				"mac": "cmd+shift+d"
			}
		]
	}
}
